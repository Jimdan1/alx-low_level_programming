#include <stdio.h>
/**
* main - print all single digit numbers of base 10 starting from 0, followed by a new line
*
* Description : using main funtion
* This program is a script to print all single digit numbers of base 10 starting form 0
* return: 0
*/
int main(void)
{
int d;
for (d = '0' ; d <= '9' d++)
{
putchar(d);
}
putchar('\n);
return (0);
}
