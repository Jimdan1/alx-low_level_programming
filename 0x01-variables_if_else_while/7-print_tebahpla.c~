#include <stdio.h>
/**
* main - prints the lowercase alphabet in reverse order
* Description: main function
* This program prints the lowercase alphabet in reverse order
* return: 0
*/
int main(void)
{
char c;
for (c = '0'; c <= '9'; c++)
{
putchar(c);
}
for (c = 'a'; c <= 'f'; c++)
{
putchar(c);
}
putchar('\n');
return (0);
}
